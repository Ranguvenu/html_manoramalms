<div class="calendar_events attempt_text">
    <div class="header_section">
        <div class="attempt_textimg">
            <!-- <i class="fa fa-sticky-note-o" aria-hidden="true"></i> -->
            <span class="icon"></span>

        </div>
        <div class="header_content align-items-center">
            <h5 class="m-0"><span>{{typeofevent}}</span>:<span>{{name}}</span></h5>
            <div class="missed_status">Missed</div>
        </div>

    </div>
    <div class="row calendar_events_content">
        <div class="col-md-3">
            <div class="dateinfo">
                <div class="labels text-muted">{{#str}}date, block_notification{{/str}}</div>
                <div class="datainfo">{{#datestart}} {{datestart}}{{/datestart}}</div>
            </div>
            <div class="timeinfo">
                <div class="labels text-muted">{{#str}}time, block_notification{{/str}}</div>
                <div class="datainfo">{{#timestart}} {{timestart}}{{/timestart}}</div>
            </div>
        </div>
        <div class="col-md-3">
            {{#duration}}
            <div class="duration">
                <div class="labels text-muted">{{#str}}duration, block_notification{{/str}}</div>
                <div class="datainfo">{{duration}}</div>
            </div>
             <!-- <div class="typeinfo">
                <div class="labels text-muted">Type</div>
                <div class="datainfo">{{typeofevent}}</div>
            </div>    -->
       {{/duration}}
        </div>
        <div class="col-md-3">
            {{#isquiz}}
            <div class="questionscount">
                <div class="labels text-muted">{{#str}}noofquestions, block_notification{{/str}}</div>
                <div class="datainfo">{{qcount}} {{#str}}questions, block_notification{{/str}}</div>
            </div>
            <div class="marks">
                <div class="labels text-muted">{{#str}}marks, block_notification{{/str}}</div>
                <div class="datainfo">{{qmarks}} {{#str}}marks, block_notification{{/str}}</div>
            </div>            
            {{/isquiz}}
        </div>
        {{#isquiz}}
        <div class="col-md-3">
            <a href="{{startattempturl}}"><button class="text_btn">{{#str}}begintest, block_notification{{/str}}</button></a>
        </div>
        {{/isquiz}}
        {{^isquiz}}
        <div class="col-md-3">
            <a href="{{startattempturl}}"><button class="text_btn">{{#str}}start, block_notification{{/str}}</button></a>
        </div>
        {{/isquiz}}
       <!--  <div class="col-md-3">
            <a href="{{startattempturl}}"><button class="answerkey_btn">{{#str}}answerkey, block_notification{{/str}}</button></a>
        </div>
        <div class="col-md-3">
            <a href="{{startattempturl}}"><button class="scorecard_btn">{{#str}}scorecard, block_notification{{/str}}</button></a>
        </div> -->

    </div> 
    
</div>
